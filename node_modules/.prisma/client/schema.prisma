// schema.prisma

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("postgresql://login:password@localhost:5432/db")
}

model Itvshow {
  id          Int      @id @default(autoincrement())
  code        Int
  name        String
  director    String
  investment  Int
  cast        Int
  version     Int
}
